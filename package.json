{
	"name": "home",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"prestart": "node ./generate.js",
		"start": "craco start",
		"build": "craco build",
		"test": "craco test",
		"eject": "react-scripts eject",
		"postinstall": "npm run simple-git-hooks",
		"simple-git-hooks": "simple-git-hooks",
		"format": "eslint --fix --config '.eslintrc' --ignore-path '.eslintignore' --ext ts,tsx  && prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
		"lint-staged": "lint-staged"
	},
	"dependencies": {
		"@ant-design/icons": "^4.7.0",
		"@apollo/client": "^3.5.6",
		"@craco/craco": "^6.4.3",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.0.3",
		"@types/node": "^17.0.1",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@types/styled-components": "^5.1.18",
		"antd": "^4.17.3",
		"apollo-link-rest": "^0.8.0",
		"constate": "^3.3.0",
		"craco-antd": "^2.0.0",
		"craco-graphql-loader": "^1.0.1",
		"firebase": "^9.8.3",
		"graphql": "^16.2.0",
		"graphql-anywhere": "^4.2.7",
		"lint-staged": "^12.1.3",
		"prettier": "^2.5.1",
		"qs": "^6.10.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-error-overlay": "^6.0.9",
		"react-router-dom": "^6.2.1",
		"react-scripts": "^4.0.3",
		"source-map-explorer": "^2.5.2",
		"styled-components": "^5.3.3",
		"typescript": "^4.5.4",
		"web-vitals": "^2.1.2"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.7.0",
		"@typescript-eslint/parser": "^5.7.0",
		"craco-alias": "^3.0.1",
		"eslint-config-airbnb": "^19.0.2",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"sass-variable-parser": "^1.2.2",
		"simple-git-hooks": "^2.7.0",
		"typescript-interface-generator": "0.0.6"
	},
	"simple-git-hooks": {
		"pre-commit": "npm run lint-staged"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"eslint --fix --config '.eslintrc' --ignore-path '.eslintignore'"
		],
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --write"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
